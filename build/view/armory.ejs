  <label for="armory_name"><span class="lbl">Name:</span><input id="armory_name" type="input" value="<%= character.attr('armory_name') %>" /></label>
  <label for="realm"><span class="lbl">Realm:</span><input id="armory_realm" type="input" value="<%= character.attr('armory_realm') %>" /></label>
  <label for="region"><span class="lbl">Region:</span><input id="armory_region" type="input" value="<%= character.attr('armory_region') %>" /></label>
  <label class="newLine long"><span class="lbl">Known characters:</span>
  <select>
    <% can.each(past_searches, function(search) { %>
      <option value="<%= search %>"><%= search %></option>
    <% }) %>
  </select>
  </label>
  <input id="armory_btn" type="button" value="OK" />
