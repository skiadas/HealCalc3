<div <%= (el) -> can.data(el, 'stat', stats) %> >
<label><span class="lbl">Intellect</span><input type="input" id="bint" value="<%= stats.attr('bint'); %>" /></label>
<label><span class="lbl">Spirit</span><input type="input" id="bspi" value="<%= stats.attr('bspi'); %>" /></label>
<label><span class="lbl">Crit Rating</span><input type="input" id="bcrit" value="<%= stats.attr('bcrit'); %>" /></label>
<label><span class="lbl">Haste Rating</span><input type="input" id="bhaste" value="<%= stats.attr('bhaste'); %>" /></label>
<label><span class="lbl">Mastery Rating</span><input type="input" id="bmast" value="<%= stats.attr('bmast'); %>" /></label>
<label><span class="lbl">Multistrike Rating</span><input type="input" id="bmulti" value="<%= stats.attr('bmulti'); %>" /></label>
</div>
<div>
<label><span class="lbl">Versatility Rating</span><input type="input" id="bvers" value="<%= stats.attr('bvers'); %>" /></label>
<label><span class="lbl">Weapon Sp</span><input type="input" id="bweapon" value="<%= stats.attr('bweapon'); %>" /></label>
<label><span class="lbl">Weapon Damage</span><input type="input" id="bwdam" value="<%= stats.attr('bwdam'); %>" /></label>
<label><span class="lbl">Weapon Base Speed</span><input type="input" id="bwspeed" value="<%= stats.attr('bwspeed'); %>" /></label>
<label><span class="lbl">Weapon Base Dps</span><input type="input" id="bwdps" value="<%= stats.attr('bwdps'); %>" /></label>
</div>
