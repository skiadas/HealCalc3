<div <%= (el) -> can.data(el, 'stat', stats) %> ><a class="expandOn"><h4>Stats</h4></a>
<div>
<label>Intellect<input type="input" id="bint" value="<%= stats.attr('bint'); %>" /></label>
<label>Spirit<input type="input" id="bspi" value="<%= stats.attr('bspi'); %>" /></label>
<label>Crit Rating<input type="input" id="bcrit" value="<%= stats.attr('bcrit'); %>" /></label>
<label>Haste Rating<input type="input" id="bhaste" value="<%= stats.attr('bhaste'); %>" /></label>
<label>Mastery Rating<input type="input" id="bmast" value="<%= stats.attr('bmast'); %>" /></label>
<label>Weapon Sp<input type="input" id="bweapon" value="<%= stats.attr('bweapon'); %>" /></label>
<label>Weapon Base Speed<input type="input" id="bwspeed" value="<%= stats.attr('bwspeed'); %>" /></label>
<label>Weapon Base Dps<input type="input" id="bwdps" value="<%= stats.attr('bwdps'); %>" /></label>
</div>
</div>
