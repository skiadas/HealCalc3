<% specs.each(function(spec) { %>
  <div class="spec" <%= (el) -> can.data(el, 'spec', spec) %> >
    <%== ImageLoader(spec.icon) %>
    <a class="expandOn"><h4><%= spec.name %></h4></a>
    <div>
      <ul><li><a class="expandOff">Settings</a>
      <div class="settings">
      <%== can.view.render(['specs',spec.name,'View'].join(''), {spec: spec}) %>
      </div></li>
      <li><a class="expandOff">Stats</a>
      <div class="stats">
        <label>Intellect<input type="input" id="int" value="<%= spec.attr('int'); %>" readonly /></label>
        <label>Spirit<input type="input" id="spi" value="<%= spec.attr('spi'); %>" readonly /></label>
        <label>Crit Percent<input type="input" id="crit" value="<%= spec.attr('crit'); %>" readonly /></label>
        <label>Haste Percent<input type="input" id="haste" value="<%= spec.attr('haste'); %>" readonly /></label>
        <label>Mastery Percent<input type="input" id="mast" value="<%= spec.attr('mast'); %>" readonly /></label>
        <label>Spellpower<input type="input" id="sp" value="<%= spec.attr('sp'); %>" readonly /></label>
    <% if (spec.name === 'Monk') { %>
        <label>Melee Haste<input type="input" id="monk_melee_haste" value="<%= spec.attr('monk_melee_haste'); %>" readonly /></label>
        <label>Weapon Speed<input type="input" id="monk_wspeed" value="<%= spec.attr('monk_wspeed'); %>" readonly /></label>
        <label>Weapon DPS<input type="input" id="monk_wdps" value="<%= spec.attr('monk_wdps'); %>" readonly /></label>
        <label>Weapon Damage<input type="input" id="monk_wdam" value="<%= spec.attr('monk_wdam'); %>" readonly /></label>
    <% } %>
      </div></li>
      <li><a class="expandOff">Haste Points</a>
        <div <%= (el) -> can.data(el, 'spec', spec) %> class="haste_bar_cont" id="haste_bar_<%= spec.attr('name'); %>" >
        </div></li>
      <li><a class="expandOff">Mana Regen</a>
        <div class="mana_regen">
          <label>Time duration<input type="input", name="interval_time" value="<%=spec.attr('interval_time'); %>" /></label>
          <table>
            <thead>
            <tr><th>Source</th><th>MPS</th><th>Mana</th></tr>
            </thead>
            <tbody>
              <% spec.mana_sources.each(function(source) { %>
                <tr><td><%= source.name %></td><td><%= source.attr('mps') %></td><td><%= source.attr('mana') %></td></tr>
              <% }) %>
              <tr class="total"><td>Total</td><td><%= spec.attr('regen_mps') %></td><td><%= spec.attr('regen_mana') %></td>
            </tbody>
          </table>
        </div></li>
      </ul>
    </div>
  </div>
<% }) %>